<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/titlelayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >
    </LinearLayout>
    <View
        style="@style/line_gray_night"
        android:layout_marginLeft="40dp" />
   
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/first_height_52.5"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingBottom="4dp"
        android:paddingLeft="5dp"
        android:onClick="ToCommission"
        android:paddingRight="@dimen/margin_10"
        android:paddingTop="4dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/instructions"
            android:drawablePadding="10dp"
            android:gravity="center_vertical"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            android:text="@string/diaoce"
            android:textSize="@dimen/textsize_19" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/input_area"
            android:contentDescription="@string/about_us"
            android:layout_gravity="right|center_vertical"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical" />
    </LinearLayout>

    <View
        style="@style/line_gray_night"
        android:layout_marginLeft="40dp" />

   
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/first_height_52.5"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingBottom="4dp"
        android:paddingLeft="5dp"
        android:onClick="TO_DT_MODE_CONF"
        android:paddingRight="@dimen/margin_10"
        android:paddingTop="4dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/instructions"
            android:drawablePadding="10dp"
            android:gravity="center_vertical"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            android:text="@string/dt_mode_conf"
            android:textSize="@dimen/textsize_19" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/input_area"
            android:contentDescription="@string/about_us"
            android:layout_gravity="right|center_vertical"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical" />
    </LinearLayout>
    
    <View
        style="@style/line_gray_night"
        android:layout_marginLeft="40dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/first_height_52.5"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingBottom="4dp"
        android:paddingLeft="5dp"
        android:onClick="ToOAM"
        android:paddingRight="@dimen/margin_10"
        android:paddingTop="4dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/instructions"
            android:drawablePadding="10dp"
            android:gravity="center_vertical"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            android:text="@string/oam"
            android:textSize="@dimen/textsize_19" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/input_area"
            android:contentDescription="@string/about_us"
            android:layout_gravity="right|center_vertical"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical" />
    </LinearLayout>
    
    <View
        style="@style/line_gray_night"
        android:layout_marginLeft="40dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/first_height_52.5"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingBottom="4dp"
        android:paddingLeft="5dp"
        android:onClick="ToLteActivateCAC"
        android:paddingRight="@dimen/margin_10"
        android:paddingTop="4dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/instructions"
            android:drawablePadding="10dp"
            android:gravity="center_vertical"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            android:text="@string/lteActivateCAC"
            android:textSize="@dimen/textsize_19" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/input_area"
            android:contentDescription="@string/about_us"
            android:layout_gravity="right|center_vertical"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical" />
    </LinearLayout>    
    <View
        style="@style/line_gray_night"
        android:layout_marginLeft="40dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/first_height_52.5"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingBottom="4dp"
        android:paddingLeft="5dp"
        android:onClick="ToSMS"
        android:paddingRight="@dimen/margin_10"
        android:paddingTop="4dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/instructions"
            android:drawablePadding="10dp"
            android:gravity="center_vertical"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            android:text="@string/sms_test"
            android:textSize="@dimen/textsize_19" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/input_area"
            android:contentDescription="@string/about_us"
            android:layout_gravity="right|center_vertical"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical" />
    </LinearLayout>
    
    
    <View
        style="@style/line_gray_night"
        android:visibility="gone"
        android:layout_marginLeft="40dp" />

   
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/first_height_52.5"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingBottom="4dp"
        android:paddingLeft="5dp"
        android:onClick="ToRSA"
        android:visibility="gone"
        android:paddingRight="@dimen/margin_10"
        android:paddingTop="4dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/instructions"
            android:drawablePadding="10dp"
            android:gravity="center_vertical"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            android:text="@string/rsa"
            android:textSize="@dimen/textsize_19" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/input_area"
            android:contentDescription="@string/about_us"
            android:layout_gravity="right|center_vertical"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical" />
    </LinearLayout>

    <View
        style="@style/line_gray_night"
        android:layout_marginLeft="40dp" />
    
     <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/first_height_52.5"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingBottom="4dp"
        android:onClick="ToCloseDevice"
        android:paddingLeft="5dp"
        android:paddingRight="@dimen/margin_10"
        android:paddingTop="4dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/exit"
           android:drawablePadding="10dp"
           android:layout_marginLeft="5dp"
            android:gravity="center_vertical"
            android:layout_weight="1"
            android:text="@string/close_device"
            android:textSize="@dimen/textsize_19" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/input_area"
            android:contentDescription="@string/about_us"
            android:layout_gravity="right|center_vertical"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical" />
    </LinearLayout>

    <View
        style="@style/line_gray_night"
        android:layout_marginLeft="40dp" />

</LinearLayout>