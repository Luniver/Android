                            android:layout_toRightOf="@+id/rl_seq1"
                            android:paddingLeft="15dp" >

                            <EditText
                                android:id="@+id/et_frequency1"
                                android:layout_width="110.5dp"
                                android:layout_height="38.5dp"
                                android:layout_gravity="center"
                                android:background="@drawable/search_input_unavailable"
                                android:clickable="false"
                                android:enabled="false"
                                android:focusable="false"
                                android:inputType="number"
                                android:maxLength="3"
                                android:paddingLeft="4dp"
                                android:textColor="@color/textcolor_frequency_gray"
                                android:textSize="16sp" />

                            <ImageView
                                android:id="@+id/iv_clear_frequency1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:layout_marginRight="5dp"
                                android:background="@drawable/search_clear"
                                android:onClick="onClearFreClick"
                                android:visibility="gone" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <!-- 第二个频点 -->

                    <RelativeLayout
