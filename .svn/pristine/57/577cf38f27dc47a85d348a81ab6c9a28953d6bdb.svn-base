                        </RelativeLayout>
                    </RelativeLayout>

                    <!-- 第七个频点 -->

                    <RelativeLayout
                        android:id="@+id/rl_input7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingBottom="7.5dp"
                        android:paddingRight="14dp" >

                        <!-- 序号 -->

                        <RelativeLayout
                            android:id="@+id/rl_seq7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true" >

                            <TextView
                                android:id="@+id/tv_seq7"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/search_seq_unactivate" />

                            <TextView
                                android:id="@+id/tv_7"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:text="@string/rl_seq7"
                                android:textColor="@color/textcolor_frequency_gray"
                                android:textSize="24sp" />
                        </RelativeLayout>
                        <!-- 频点 -->

                        <RelativeLayout
                            android:id="@+id/rl_frequency7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@+id/rl_seq7"
                            android:paddingLeft="15dp" >

                            <EditText
                                android:id="@+id/et_frequency7"
                                android:layout_width="110.5dp"
                                android:layout_height="38.5dp"
                                android:layout_gravity="center"
                                android:background="@drawable/search_input_unavailable"
                                android:clickable="false"
                                android:enabled="false"
                                android:focusable="false"
                                android:inputType="number"
                                android:maxLength="3"
                                android:paddingLeft="4dp"
                                android:textColor="#ffffff"
                                android:textSize="16sp" />

                            <ImageView
                                android:id="@+id/iv_clear_frequency7"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:layout_marginRight="5dp"
                                android:background="@drawable/search_clear"
                                android:onClick="onClearFreClick"
                                android:visibility="gone" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <!-- 第八个频点 -->

                    <RelativeLayout
                        android:id="@+id/rl_input8"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingBottom="7.5dp"
                        android:paddingRight="14dp" >

                        <!-- 序号 -->

                        <RelativeLayout
                            android:id="@+id/rl_seq8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true" >

                            <TextView
                                android:id="@+id/tv_seq8"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/search_seq_unactivate" />

                            <TextView
                                android:id="@+id/tv_8"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:text="@string/rl_seq8"
                                android:textColor="@color/textcolor_frequency_gray"
                                android:textSize="24sp" />
                        </RelativeLayout>
                        <!-- 频点 -->

                        <RelativeLayout
                            android:id="@+id/rl_frequency8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@+id/rl_seq8"
                            android:paddingLeft="15dp" >

                            <EditText
                                android:id="@+id/et_frequency8"
                                android:layout_width="110.5dp"
                                android:layout_height="38.5dp"
                                android:layout_gravity="center"
                                android:background="@drawable/search_input_unavailable"
                                android:clickable="false"
                                android:enabled="false"
                                android:focusable="false"
                                android:inputType="number"
                                android:maxLength="3"
                                android:paddingLeft="4dp"
                                android:textColor="#ffffff"
                                android:textSize="16sp" />

                            <ImageView
                                android:id="@+id/iv_clear_frequency8"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:layout_marginRight="5dp"
                                android:background="@drawable/search_clear"
                                android:onClick="onClearFreClick"
                                android:visibility="gone" />
                        </RelativeLayout>
                    </RelativeLayout>
                </LinearLayout>
            </ScrollView>
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_blind_operate"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:visibility="visible" >

        <View
            android:id="@+id/line"
            style="@style/above_button_line" />

        <Button
            android:id="@+id/tv_blind_search_start"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/line"
            android:background="@drawable/button_selector"
            android:gravity="center"
            android:onClick="onSearchClick"
            android:text="@string/btn_blindstartsearch"
            android:textColor="#ffffff"
            android:textSize="@dimen/textsize_19" />
    </RelativeLayout>

    <Chronometer
        android:id="@+id/search_chorm"
        android:layout_width="0dp"
        android:layout_height="0dp" />

</RelativeLayout>