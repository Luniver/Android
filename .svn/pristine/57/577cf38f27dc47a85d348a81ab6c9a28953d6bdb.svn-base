<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <!-- 顶部-搜索方式 -->

    <RelativeLayout
        android:id="@+id/rl_title"
        android:layout_width="match_parent"
        android:layout_height="51dp"
        android:layout_alignParentTop="true"
        android:background="#03214e" >

        <LinearLayout
            android:id="@+id/titlelayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >
        </LinearLayout>

        <TextView
            android:id="@+id/tv_search_sum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/tv_search_unit"
            android:paddingRight="3dp"
            android:text="0"
            android:textColor="#03fc36"
            android:visibility="invisible"
            android:textSize="@dimen/textsize_19" />

        <TextView
            android:id="@+id/tv_search_unit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:paddingRight="10dp"
            android:text="@string/tv_search_unit"
            android:visibility="invisible"
            android:textColor="#ffffff"
            android:textSize="@dimen/textsize_19" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_search_format"
        android:layout_width="match_parent"
        android:layout_height="107.5dp"
        android:layout_below="@+id/rl_title" >

        <ImageView
            android:id="@+id/iv_signal_picture"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:contentDescription="@string/about_us"
            android:padding="10dp"
            android:src="@drawable/search_basestation" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_toRightOf="@+id/iv_signal_picture"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/rl_current_format"
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1" >

                <TextView
                    android:id="@+id/tv_current_format"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="@string/tv_currentformat"
                    android:textSize="@dimen/textsize_17" />

                <TextView
                    android:id="@+id/tv_gsm_cmcc"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingRight="10dp"
                    android:text="@string/gsm_cmcc"
                    android:textColor="#52A4D9"
                    android:textSize="@dimen/textsize_17" />
            </LinearLayout>

            <View
                android:id="@+id/v_divide_format_center"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#333333" />

            <RadioGroup
                android:id="@+id/rg_search"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <RadioButton
                    android:id="@+id/rb_blind"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@null"
                    android:button="@null"
                    android:checked="true"
                    android:drawableLeft="@drawable/radio_style"
                    android:drawablePadding="8dp"
                    android:gravity="center_vertical"
                    android:text="@string/tv_blindsearch" />

                <RadioButton
                    android:id="@+id/rb_fixed"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@null"
                    android:button="@null"
                    android:drawableLeft="@drawable/radio_style"
                    android:drawablePadding="8dp"
                    android:gravity="center_vertical"
                    android:text="@string/tv_fixedsearch" />
            </RadioGroup>
        </LinearLayout>

        <View
            android:id="@+id/v_divide_format_bottom"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="#333333" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_fixed_info"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/rl_search_format"
        android:paddingTop="2dp"
        android:visibility="gone"    >

        <RelativeLayout
            android:id="@+id/rl_fixed_title"
            android:layout_width="match_parent"
            android:layout_height="27.5dp"
            android:background="#03444E" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:paddingLeft="10dp"
                android:text="@string/tv_frequency_param"
                android:textColor="#ffffff"
                android:textSize="@dimen/textsize_15" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/tv_activate_sum"
                android:text="@string/tv_activated"
                android:textColor="#ffffff"
                android:textSize="@dimen/textsize_15" />

            <TextView
                android:id="@+id/tv_activate_sum"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/tv_Activate_Unit"
                android:paddingRight="3dp"
                android:text="3"
                android:textColor="#04FC38"
                android:textSize="@dimen/textsize_15" />

            <TextView
                android:id="@+id/tv_Activate_Unit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:paddingRight="10dp"
                android:text="@string/tv_activate_unit"
                android:textColor="#ffffff"
                android:textSize="@dimen/textsize_15" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_fixed_operate"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_alignParentBottom="true"
            android:visibility="visible" >

            <View
                android:id="@+id/line2"
                style="@style/above_button_line" />

            <LinearLayout
                android:id="@+id/ll_btn"
                android:layout_width="match_parent"
                android:layout_height="@dimen/first_height_52.5"
                android:layout_below="@+id/line2"
                android:background="@drawable/button_selector"
                android:orientation="horizontal" >

                <Button
                    android:id="@+id/tv_clear_all_frequency"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/button_selector"
                    android:gravity="center"
                    android:height="@dimen/first_height_52.5"
                    android:onClick="onClearAllClick"
                    android:text="@string/tv_clear_frequency_point"
                    android:textColor="#ffffff"
                    android:textSize="@dimen/textsize_19" />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="6dp"
                    android:layout_marginTop="6dp"
                    android:background="#AEB1B1" />

                <Button
                    android:id="@+id/tv_fixed_search_start"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/first_height_52.5"
                    android:layout_weight="1"
                    android:background="@drawable/button_selector"
                    android:gravity="center"
                    android:height="50dp"
                    android:onClick="onSearchClick"
                    android:text="@string/btn_fixedstartsearch"
                    android:textColor="#ffffff"
                    android:textSize="@dimen/textsize_19" />
            </LinearLayout>
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/ll_fixed_info"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/rl_fixed_operate"
            android:layout_below="@+id/rl_fixed_title"
            android:layout_marginTop="6dp"
            android:baselineAligned="false"
            android:orientation="horizontal" >

            <LinearLayout
                android:id="@+id/ll_blind_result"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/ll_blind_result_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#03444e"
                    android:padding="10dp" >

                    <TextView
                        android:id="@+id/tv_seq"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/tv_serial_number"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/textsize_16" />

                    <TextView
                        android:id="@+id/tv_frequency"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/tv_frequency_point"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/textsize_16" />

                    <TextView
                        android:id="@+id/tv_signal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/tv_signal_strength"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/textsize_16" />
                </LinearLayout>

                <ListView
                    android:id="@+id/listview_blind_result"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:divider="@drawable/line_gray"
                    android:scrollbars="none" >
                </ListView>
            </LinearLayout>

            <!-- 输入频点 -->
