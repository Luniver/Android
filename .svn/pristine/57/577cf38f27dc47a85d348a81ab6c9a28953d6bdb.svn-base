                            <TextView
                                android:id="@+id/tv_seq1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/search_seq_unactivate" />

                            <TextView
                                android:id="@+id/tv_1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:text="@string/rl_seq1"
                                android:textColor="@color/textcolor_frequency_gray"
                                android:textSize="24sp" />
                        </RelativeLayout>
                        <!-- 频点 -->

                        <RelativeLayout
                            android:id="@+id/rl_frequency1"
                            android:layout_width="wrap_content"
