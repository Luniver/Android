package hrst.sczd.agreement.manager.vo;

public class QueryTfKeepNumberFile {
	
	/* 参数          n: 全部文件个数
	 * 参数	n>0: 第n个文件

		       文件名	  文件名字符(不含路径)	20字节*/

	private short totalFile;
	
	private short fileNum;
	
	private String fileName = "";

	public short getTotalFile() {
		return totalFile;
	}

	public void setTotalFile(short totalFile) {
		this.totalFile = totalFile;
	}

	public short getFileNum() {
		return fileNum;
	}

	public void setFileNum(short fileNum) {
		this.fileNum = fileNum;
	}

	public String getFileName() {
		return fileName;
	}

	public void setFileName(String fileName) {
		this.fileName = fileName;
	}

	@Override
	public String toString() {
		return "QueryTfKeepNumberFile [totalFile=" + totalFile + ", fileNum="
				+ fileNum + ", fileName=" + fileName + "]";
	}

}
