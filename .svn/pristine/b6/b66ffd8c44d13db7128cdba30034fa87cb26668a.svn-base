<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:xiaoya="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_dialog_color"
    android:orientation="vertical" >
    
    <TextView 
        android:id="@+id/txt_keep_num_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center_horizontal"
        android:textColor="@color/white"
        android:textSize="@dimen/textsize_15"
        android:text="@string/dialog_hint"/>
    
    <hrst.sczd.view.HorizontalProgressBarWithProgress
        android:id="@+id/progress_keep_num"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="20dp"
        xiaoya:progress_text_color="#ffff0000"
        xiaoya:progress_reach_color="#ffff0000"
        xiaoya:progress_unreach_color="#55ff0000"/>
    
    <Button 
        android:id="@+id/btn_keep_number_cancel"
        android:layout_width="60dp"
        android:layout_height="30dp"
        android:layout_marginTop="20dp"
        android:layout_gravity="center_horizontal"
        android:text="@string/cancel"
        android:visibility="gone"
        android:textColor="@color/white"
        android:textSize="@dimen/textsize_16"
        android:background="@drawable/btn_cancel_sczd"/>

</LinearLayout>
