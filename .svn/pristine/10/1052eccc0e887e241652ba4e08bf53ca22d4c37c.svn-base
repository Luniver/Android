package hrst.sczd.agreement.sczd.vo;

/**
 * 设备主动上报温度/电池容量状态
 * @author glj
 * 2018-01-15
 */
public class MCU_TO_PDA_TEMPERATURE_AND_ELECTRICITY_STATE {
	
/*	参数1	数字版温度值	2字节
 *  参数2	RF温度值	2字节
	参数3	电量值	2字节*/
	
	private short digitalTemperature_a;
	
	private short rFTemperature_b;
	
	private short electricity_c;


	public short getDigitalTemperature_a() {
		return digitalTemperature_a;
	}


	public void setDigitalTemperature_a(short digitalTemperature_a) {
		this.digitalTemperature_a = digitalTemperature_a;
	}


	public short getrFTemperature_b() {
		return rFTemperature_b;
	}


	public void setrFTemperature_b(short rFTemperature_b) {
		this.rFTemperature_b = rFTemperature_b;
	}


	public short getElectricity_c() {
		return electricity_c;
	}


	public void setElectricity_c(short electricity_c) {
		this.electricity_c = electricity_c;
	}


	@Override
	public String toString() {
		return "MCU_TO_PDA_TEMPERATURE_AND_ELECTRICITY_STATE [digitalTemperature_a="
				+ digitalTemperature_a
				+ ", rFTemperature_b="
				+ rFTemperature_b
				+ ", electricity_c=" + electricity_c + "]";
	}
	
}
